{
  "name": "sick-fits-keystone",
  "description": "An example KeystoneJS project with a User list and Authentication.",
  "private": true,
  "version": "5.0.0",
  "author": "The KeystoneJS Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "dev": "nodemon -L --exec cross-env NODE_ENV=development DISABLE_LOGGING=true NODE_OPTIONS='-r esm' keystone dev",
    "dummy": "npm run dev -- --dummy",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS='-r esm' keystone build",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS='-r esm' keystone start"
  },
  "dependencies": {
    "@keystonejs/adapter-mongoose": "^8.0.2",
    "@keystonejs/app-admin-ui": "^5.11.1",
    "@keystonejs/app-graphql": "^5.1.5",
    "@keystonejs/app-next": "^5.1.2",
    "@keystonejs/auth-password": "^5.1.6",
    "@keystonejs/build-field-types": "^5.2.5",
    "@keystonejs/fields": "^9.0.4",
    "@keystonejs/fields-datetime-utc": "^5.1.6",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.2.5",
    "@keystonejs/file-adapters": "^6.0.1",
    "@keystonejs/keystone": "^8.1.3",
    "@keystonejs/list-plugins": "^5.2.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^14.6.0",
    "graphql-fields": "^2.0.3",
    "nodemailer": "^6.4.6",
    "react": "^16.13.1",
    "stripe": "^8.46.0"
